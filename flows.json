    {
        "id": "threshold_check",
        "type": "function",
        "z": "a1a1a1a1a1a1a1a1",
        "name": "Check Thresholds",
        "func": "const moisture = msg.payload.moisture;\nconst tilt = msg.payload.tilt;\nconst moistureLimit = 60;\nconst tiltLimit = 15;\n\nmsg.status = (moisture > moistureLimit && tilt > tiltLimit) ? \"ALERT\" : \"NORMAL\";\nmsg.payload = {\n    moisture: moisture,\n    tilt: tilt,\n    status: msg.status\n};\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 360,
        "y": 100,
        "wires": [
            [
                "split_for_gauges",
                "text_status",
                "debug_log"
            ]
        ]
    },
    {
        "id": "split_for_gauges",
        "type": "function",
        "z": "a1a1a1a1a1a1a1a1",
        "name": "Split for Gauges",
        "func": "// Split moisture and tilt for separate gauges\nreturn [\n  { payload: msg.payload.moisture },\n  { payload: msg.payload.tilt }\n];",
        "outputs": 2,
